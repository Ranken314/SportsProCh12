@model Technician

@{
    string title = ViewBag.Action + "Technician";
    ViewBag.Title = title;
}



<h1>@ViewBag.Title</h1>

<form asp-action="Save" method="post">
    <div asp-validation-summary="All" class="text-danger"> </div>

    <div class="form-group">
        <label asp-for="TechnicianName">Technician Name</label>
        <input asp-for="TechnicianName" class="form-control" />
    </div>

    <div class="form-group">
        <label asp-for="TechnicianEmail">Technician Email</label>
        <input asp-for="TechnicianEmail" class="form-control" />
    </div>


    <div class="form-group">
        <label asp-for="TechnicianPhone">Technician Phone</label>
        <input asp-for="TechnicianPhone" class="form-control" />
    </div>

    <input type="hidden" asp-for="TechnicianID" />

    <button type="submit" class="btn btn-primary">Save</button>
    <a asp-action="List" class="btn btn-primary">Cancel</a>

</form>
